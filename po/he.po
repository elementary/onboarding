msgid "Banana"
msgstr "בננה"

#: src/Views/StyleView.vala:178
msgid "Orange"
msgstr "תפוז"

#: src/Views/StyleView.vala:183
msgid "Strawberry"
msgstr "תות"

#: src/Views/StyleView.vala:188
msgid "Bubblegum"
msgstr "מסטיק"

#: src/Views/StyleView.vala:193
msgid "Grape"
msgstr "ענב"

#: src/Views/StyleView.vala:198
msgid "Cocoa"
msgstr "קקאו"

